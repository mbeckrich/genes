
{{- $regex := findRE `(?mi)^[0-9]+\.\s+([^.])+\.` .RawContent -}}
{{- $ahnentafel := slice -}}

  {{- $counter := 0 -}}
  {{- range where site.RegularPages "Type" "ahnentafel" }}
  {{- range $index, $name := $regex -}}

   {{- $generation := (math.Floor (div (math.Log ($counter = add $counter 1)) (math.Log 2)))
   -}}
   {{- $num := 0 -}}
   {{- $id := add $num $counter -}}
   {{- if lt $index 9 -}}
   {{- $id = print "-0" $id -}}
   {{- else -}}
   {{- $id = print "-0" $id -}}
   {{- end -}}

   {{- $generation = string $generation -}}
   {{- $id := print "[0" $generation print $id "]" -}}

   {{- $dict := dict
   $name $id -}}

  {{- $ahnentafel = $ahnentafel | append $dict -}}
  {{- end -}}
  {{- end -}}

{{- $object := dict "ahnentafel" $ahnentafel -}}