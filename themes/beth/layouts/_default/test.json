{{- $ahnentafel := slice -}}
{{- $rc := .RawContent -}}
{{- $regex_names := findRE `(?mi)^[0-9]+\.\s+([^.])+` $rc -}}
{{- $regex_birthdate :=  $rc -}}

{{- $counter := 0 -}}
{{- range where site.RegularPages "Section" "reference-lists" -}}
  {{- range $index, $value := $regex_names -}}

    {{- $name := replaceRE `[0-9.]+\s+` "" $value -}}

    {{- $generation := (math.Floor (div (math.Log ($counter = add $counter 1)) (math.Log 2))) -}}
    {{- $num := 0 -}}
    {{- $id := add $num $counter -}}

      {{- if lt $index 9 -}}
      {{- $id = print "-00" $id -}}
      {{- else -}}
      {{- $id = print "-0" $id -}}
      {{- end -}}

    {{- $generation = string $generation -}}
    {{- $id := print "[0" $generation print $id "]" -}}

    {{- $ahnentafel = merge $ahnentafel
      (dict
      (slice $id "name") $name
      (slice $id "birth date") "bday"
      (slice $id "birthplace") "hell"
      (slice $id "diedplace") "heaven"
      )
      -}}
  {{- end -}}
{{- end -}}

{{- $object := dict "ahnentafel" $ahnentafel -}}
{{- $object | jsonify -}}

